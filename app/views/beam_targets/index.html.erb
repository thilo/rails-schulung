<h1>Listing beam_targets</h1>

<table>
  <tr>
    <th>Density</th>
    <th>Volume</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @beam_targets.each do |beam_target| %>
  <tr>
    <td><%= beam_target.density %></td>
    <td><%= beam_target.volume %></td>
    <td><%= link_to 'Show', beam_target %></td>
    <td><%= link_to 'Edit', edit_beam_target_path(beam_target) %></td>
    <td><%= link_to 'Destroy', beam_target, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Beam target', new_beam_target_path %>
